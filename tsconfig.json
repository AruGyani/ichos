{
    "compilerOptions": {
        "module": "commonjs",
        "baseUrl": ".",
        "esModuleInterop": true,
        "target": "es6",
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "rootDir": "src",
        "paths": {
            "@discordjs/voice": ["../../"],
			"@discordjs/opus": ["./node_modules/@discordjs/opus"],
			"sodium": ["./node_modules/sodium"],
			"libsodium-wrappers": ["./node_modules/libsodium-wrappers"],
			"tweetnacl": ["./node_modules/tweetnacl"]
        }
    },
    "include": ["src/**/*.ts"],
    "exclude": [""],
    "lib": ["es6"]
}